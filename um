//crie uma função capaz de receber 3 números a, b e c, e retornar o delta desses números
#include <stdio.h>

double calcularDelta(double a, b, c) {
    return (b * b) - (4 * a * c);
}

int main() {
    double a, b, c;
    scanf("%lf %lf %lf", &a, &b, &c);

    double delta = calcularDelta(a, b, c);
    printf("%lf\n", delta);

    return 0;
}
