//Faça uma função capaz de receber dois números b e h que representam os lados de um retângulo ou quadrado, e retornar 1 se ele for um quadrado ou 0 se for um retângulo.
#include <stdio.h>

int quadrado_ou_retangulo(int b, int h) {
    if (b == h) return 1;
    return 0;
}

int main() {
    int b, h;
    scanf("%d %d", &b, &h);
    printf("%d\n", quadrado_ou_retangulo(b, h));
    return 0;
}
